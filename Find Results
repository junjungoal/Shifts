    1  require 'pathname'
    2: require 'hike/extensions'
    3: require 'hike/index'
    4: require 'hike/paths'
    5  
    6: module Hike
    7    # `Trail` is the public container class for holding paths and extensions.
    8    class Trail
    9      # `Trail#paths` is a mutable `Paths` collection.
   10      #
   11:     #     trail = Hike::Trail.new
   12:     #     trail.paths.push "~/Projects/hike/lib", "~/Projects/hike/test"
   13      #
   14      # The order of the paths is significant. Paths in the beginning of
   15      # the collection will be checked first. In the example above,
   16:     # `~/Projects/hike/lib/hike.rb` would shadow the existent of
   17:     # `~/Projects/hike/test/hike.rb`.
   18      attr_reader :paths
   19  
   20      # `Trail#extensions` is a mutable `Extensions` collection.
   21      #
   22:     #     trail = Hike::Trail.new
   23:     #     trail.paths.push "~/Projects/hike/lib"
   24      #     trail.extensions.push ".rb"
   25      #
   ..
   32      # an `Array` of aliases.
   33      #
   34:     #   trail = Hike::Trail.new
   35:     #   trail.paths.push "~/Projects/hike/site"
   36      #   trail.aliases['.htm']   = 'html'
   37      #   trail.aliases['.xhtml'] = 'html'
   ..
  105      # path collection.
  106      #
  107:     #     trail = Hike::Trail.new "~/Projects/hike"
  108      #     trail.extensions.push ".rb"
  109      #     trail.paths.push "lib", "test"
  110      #
  111:     #     trail.find "hike/trail"
  112:     #     # => "~/Projects/hike/lib/hike/trail.rb"
  113      #
  114      #     trail.find "test_trail"
  115:     #     # => "~/Projects/hike/test/test_trail.rb"
  116      #
  117      # `find` accepts multiple fallback logical paths that returns the
  118      # first match.
  119      #
  120:     #     trail.find "hike", "hike/index"
  121      #
  122      # is equivalent to
  123      #
  124:     #     trail.find("hike") || trail.find("hike/index")
  125      #
  126      # Though `find` always returns the first match, it is possible
  ...
  128      # a block.
  129      #
  130:     #     trail.find("hike", "hike/index") { |path| warn path }
  131      #
  132      # This allows you to filter your matches by any condition.
  ...
  147      #
  148      #     index = trail.index
  149:     #     index.find "hike/trail"
  150      #     index.find "test_trail"
  151      #

